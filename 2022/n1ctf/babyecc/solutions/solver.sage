#!/usr/bin/env sage

from Crypto.Util.number import long_to_bytes

ress = [[, , , ],
[, , , ],
[, , , ],
[, , , ],
[, , , ],
[, , , ],
[, , , ]]

def fun1(x, a, b):
    return (x^3 + a * x + b)

def fun2(x, a):
    return ((3 * x^2 + a)^2)

f_list = []
n_list = []
for n, a, b, y in ress:
    P.<x> = PolynomialRing(Zmod(n))
    f = (y^2 * 4^3 * fun1(x, a, b)^3) - ((fun2(x, a) - 2 * x * 4 * fun1(x, a, b))^3 + a * ((fun2(x, a) - 2 * x * 4 * fun1(x, a, b)) * 4^2 * fun1(x, a, b)^2) + b * 4^3 * fun1(x, a, b)^3)
    f_list += [f]
    n_list += [n]

ff = crt([i.change_ring(ZZ) for i in f_list], n_list)
nn = product(n_list)

ff = ff.change_ring(Zmod(nn))
x = ff.monic().small_roots(X = 2^350, beta = 0.4, epsilon = 0.01)[0] #15425251357776807541227287240467548867067510789583043568768907891381811730130189461693802496389917454461

FLAG = long_to_bytes(int(x))
print(FLAG)

